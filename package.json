{
  "name": "glide-mq",
  "version": "0.7.0",
  "description": "High-performance message queue for Node.js built on Valkey/Redis with native NAPI bindings",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "default": "./dist/testing.js"
    }
  },
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ tests/",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "rm -rf dist",
    "bench": "npx tsx benchmarks/run.ts all",
    "bench:throughput": "npx tsx benchmarks/run.ts throughput",
    "bench:latency": "npx tsx benchmarks/run.ts latency",
    "bench:memory": "npx tsx benchmarks/run.ts memory",
    "bench:sharing": "npx tsx benchmarks/run.ts sharing"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avifenesh/glide-mq.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@glidemq/speedkey": "^0.2.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^22",
    "bullmq": "^5.69.2",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "express": "^5.2.1",
    "ioredis": "^5.9.3",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "typescript-eslint": "^8.56.0",
    "vitest": "^3"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api": {
      "optional": true
    }
  },
  "keywords": [
    "queue",
    "job",
    "message",
    "valkey",
    "redis",
    "streams",
    "worker",
    "task"
  ]
}
