{
  "name": "glide-mq",
  "version": "0.4.3",
  "description": "High-performance message queue for Node.js built on Valkey/Redis with native NAPI bindings",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "default": "./dist/testing.js"
    }
  },
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist",
    "bench": "npx tsx benchmarks/run.ts all",
    "bench:throughput": "npx tsx benchmarks/run.ts throughput",
    "bench:latency": "npx tsx benchmarks/run.ts latency",
    "bench:memory": "npx tsx benchmarks/run.ts memory",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avifenesh/glide-mq.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@glidemq/speedkey": "^0.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/express": "^5.0.6",
    "@types/node": "^22",
    "@vitest/coverage-v8": "^3.0.8",
    "bullmq": "^5.69.2",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "express": "^5.2.1",
    "husky": "^9.1.7",
    "ioredis": "^5.9.3",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api": {
      "optional": true
    }
  },
  "overrides": {
    "minimatch": "^10.0.0"
  },
  "keywords": [
    "queue",
    "job",
    "message",
    "valkey",
    "redis",
    "streams",
    "worker",
    "task"
  ]
}
