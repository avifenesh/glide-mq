{
  "name": "glide-mq",
  "version": "0.0.1",
  "description": "High-performance message queue for Node.js built on Valkey/Redis with native NAPI bindings",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ tests/",
    "clean": "rm -rf dist",
    "bench": "npx tsx benchmarks/run.ts all",
    "bench:throughput": "npx tsx benchmarks/run.ts throughput",
    "bench:latency": "npx tsx benchmarks/run.ts latency",
    "bench:memory": "npx tsx benchmarks/run.ts memory"
  },
  "engines": {
    "node": ">=20"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@glidemq/speedkey": "file:../speedkey/node"
  },
  "devDependencies": {
    "@types/node": "^22",
    "bullmq": "^5.69.2",
    "ioredis": "^5.9.3",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^3"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api": {
      "optional": true
    }
  },
  "keywords": [
    "queue",
    "job",
    "message",
    "valkey",
    "redis",
    "streams",
    "worker",
    "task"
  ]
}
